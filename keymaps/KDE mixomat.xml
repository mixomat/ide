<keymap version="1" name="KDE mixomat" parent="Default for KDE">
  <action id="ChooseRunConfiguration">
    <keyboard-shortcut first-keystroke="ctrl alt released r" />
  </action>
  <action id="ExportToTextFile" />
  <action id="GotoClass">
    <keyboard-shortcut first-keystroke="ctrl n" />
    <keyboard-shortcut first-keystroke="alt o" />
  </action>
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="shift ctrl n" />
    <keyboard-shortcut first-keystroke="shift alt o" />
  </action>
  <action id="Replace">
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="ctrl r" />
  </action>
</keymap>